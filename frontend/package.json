{
  "name": "frontend",
  "version": "1.0.0",
  "description": "Frontend for the web app",
  "scripts": {
    "build": "npm run js && npm run css && npm run images",
    "js": "terser js/*.js -o dist/bundle.js",
    "css": "npx cssnano css/styles.css > dist/styles.min.css",
    "images": "mkdir -p dist/images && cp images/* dist/images/",
    "deploy": "echo 'Deploying frontend...'"
  },
  "dependencies": {
    "cssnano": "^6.0.5",
    "terser": "^5.28.1"
  },
  "devDependencies": {
  }
}